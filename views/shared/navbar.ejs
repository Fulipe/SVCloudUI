
<nav class="navbar navbar-expand-lg bg-body-tertiary">
    <div class="container-fluid justify-content-start">
      <a class="navbar-brand" href="/">filipe-cloud</a>
        <div class="navbar-nav">
          <!-- <a class="nav-link" href="#">Features</a> --> 
          <div class="navCommands">
            <ul>
                <li class="nav-item"> 
                  <a href="/navigation/createdir" class="btn btn-success" id="mkdirBtn">
                    <i class="bi bi-plus-circle"></i><b> Criar</b>
                  </a>
                </li>
                <% 
                let currentPath = ""; 
                dirAtual.forEach((dir, index) => {
                  currentPath += `/${dir}`;
                %>
                  <li class="nav-item nav-path">
                    <button type="button" class="btn btn-light">
                      <a href="/data<%= currentPath %>">
                        <h5>
                          <% if (dir == '/') { %>
                            <%= dir %>
                          <% } else { %>
                            /<%= dir %>
                          <% } %>
                        </h5>
                      </a>
                    </button> 
                  </li>
                <% }) %>               
            </ul>
          </div>
      </div>
  </div>
</nav>